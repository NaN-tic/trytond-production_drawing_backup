<form string="Production Drawing">
    <label name="name"/>
    <field name="name"/>
    <label name="active"/>
    <field name="active"/>
    <group colspan="4" id="main" yexpand="1" yfill="1">
        <group colspan="3" id="positions" yexpand="1" yfill="1">
            <field name="positions" colspan="4" yexpand="1"/>
        </group>
        <group col="1" id="image" yexpand="1">
            <field name="image" widget="image"/>
        </group>
    </group>
</form>
